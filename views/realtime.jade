doctype html
html(lang="en", ng-app="App")
	head
		meta(charset="utf-8")
		link(rel="stylesheet", href="css/vendor/bootstrap.css")
		link(rel="stylesheet", href="css/base.css")
	body
		div(class="container", ng-controller="BaseCtrl")
			div(class="row")
				div(class="col-md-12")
					div(class="panel panel-primary")
						div(class="panel-heading") Real Time
						div(class="panel-body", ng-controller="ListCtrl")
							ul(ng-repeat="obj in data")
								li {{obj.text}}
		script(type="application/javascript", src="js/vendor/angular.js")
		script(type="application/javascript", src="http://cdnjs.cloudflare.com/ajax/libs/socket.io/0.9.16/socket.io.min.js")
		script(type="application/javascript", src="js/directives.js")
		script(type="application/javascript", src="js/app.js")